<h1>Listing test_resources</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @test_resources.each do |test_resource| %>
  <tr>
    <td><%=h test_resource.name %></td>
    <td><%= link_to 'Show', test_resource %></td>
    <td><%= link_to 'Edit', edit_test_resource_path(test_resource) %></td>
    <td><%= link_to 'Destroy', test_resource, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New test_resource', new_test_resource_path %>